                        <!--                            <div class="col-md-6 pull-right">-->
                        <!--                                <span t-field="doc.company_id.arabic_vat"/>: الرقم الضريبي-->
                        <!--                            </div>-->
                        <!--                        </div>-->

                        <table width="100%"
